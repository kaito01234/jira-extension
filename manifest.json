{
  "manifest_version": 3,
  "name": "テスト拡張機能",
  "version": "0.0.1",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*.atlassian.net/jira/software/projects/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["tabs", "activeTab", "scripting"]
}
